{
  "name": "@chronocide/htmx",
  "version": "1.0.0",
  "description": "Simple static HTML generator",
  "main": "dist/htmx.cjs",
  "module": "dist/htmx.mjs",
  "types": "dist/htmx.d.ts",
  "bin": "dist/bin.js",
  "exports": {
    ".": {
      "import": "./dist/htmx.mjs",
      "require": "./dist/htmx.cjs"
    }
  },
  "repository": "https://github.com/chronoDave/htmx.git",
  "author": "Chronocide",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-transpile-only src/index.ts",
    "lint": "eslint src",
    "build": "rollup -c",
    "test": "rollup -c --noEmit && tape test/**/*.spec.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "esbuild": "^0.13.14",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-esbuild": "^4.7.2",
    "tape": "^5.3.2",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "node-html-parser": "^5.1.0"
  }
}
