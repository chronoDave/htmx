{
  "name": "@chronocide/htmx",
  "version": "1.0.0",
  "description": "Simple static HTML generator",
  "main": "dist/htmx.cjs",
  "module": "dist/htmx.mjs",
  "types": "dist/htmx.d.ts",
  "bin": {
    "htmx": "dist/bin.js"
  },
  "exports": {
    ".": {
      "import": "./dist/htmx.mjs",
      "require": "./dist/htmx.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "repository": "https://github.com/chronoDave/htmx.git",
  "author": "Chronocide",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "build": "rollup -c",
    "test": "ts-node test/bin.spec.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.9",
    "@types/yargs-parser": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "esbuild": "^0.13.14",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-esbuild": "^4.7.2",
    "tape": "^5.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "node-html-parser": "^5.1.0",
    "yargs-parser": "^21.0.0"
  }
}